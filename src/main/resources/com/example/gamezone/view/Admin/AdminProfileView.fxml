                        <ImageView fitHeight="50.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../images/logout.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <TextField fx:id="txtUsernameAdminProfile" layoutX="151.0" layoutY="250.0" prefHeight="26.0" prefWidth="314.0" promptText="Username" styleClass="textFilder" />
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="224.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/icons8-user-96.png" />
                     </image>
                  </ImageView>
                  <TextField fx:id="txtEmailAdminProfile" layoutX="151.0" layoutY="293.0" prefHeight="26.0" prefWidth="314.0" promptText="Email" styleClass="textFilder" />
                  <TextField fx:id="txtPasswordAdminProfile" layoutX="151.0" layoutY="341.0" prefHeight="26.0" prefWidth="314.0" promptText="Password" styleClass="textFilder" />
                  <Button fx:id="btnSaveAdminProfile" layoutX="270.0" layoutY="400.0" mnemonicParsing="false" onAction="#Save" prefHeight="26.0" prefWidth="60.0" styleClass="button1" text="Save">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Separator layoutY="28.0" opacity="0.2" orientation="VERTICAL" prefHeight="441.0" prefWidth="0.0" />
                  <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="131.0" layoutY="345.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/icons8-contraseña-1-96.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="22.0" fitWidth="16.0" layoutX="133.0" layoutY="299.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/icons8-mensaje-90.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="131.0" layoutY="254.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/icons8-tarjeta-de-empleado-96.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
